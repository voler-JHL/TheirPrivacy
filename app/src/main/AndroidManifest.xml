<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.voler.theirprivacy" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".SplashActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"/>
        <service android:name=".service.DaemonService"/>
        <service android:name=".service.RemoteService"/>
        <service
            android:name=".service.ServiceOne"
            android:process=":remote" >
            <intent-filter>
                <action android:name="com.example.servicedemo.ServiceOne" />
            </intent-filter>
        </service>

        <service
            android:name=".service.ServiceTwo"
            android:process=":remote" >
            <intent-filter>
                <action android:name="com.example.servicedemo.ServiceTwo" />
            </intent-filter>
        </service>
    </application>

</manifest>